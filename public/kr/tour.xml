<krpano version="1.18" title="San Marcos" onstart="js(loadScene('scene_master'));">

	<include url="skin/vtourskin.xml" />
	<include url="plugins/showtext.xml" />
 	<include url="skin/vistageneral.xml" />
   	<include url="skin/hotspots-actions.xml" />
    <include url="skin/spots.xml" />
    <include url="skin/ficha.xml" />
    <include url="skin/mapa.xml" />
	<include url="skin/vtourskin_design_117.xml" />
   <!-- <include url="../krpano/plugins/googleanalytics.xml"/> -->

	<!-- set skin settings: bingmaps? gyro? thumbnails? tooltips? -->
	<skin_settings maps="false"
	    maps_type="bing"
	    maps_bing_api_key=""
	    maps_zoombuttons="false"
	    gyro="true"
		title="true"
	    thumbs="true"
	    thumbs_width="120" thumbs_height="55" thumbs_padding="10" thumbs_crop="0|40|240|160"
	    thumbs_opened="true"
	    thumbs_text="true"
	    thumbs_dragging="true"
	    thumbs_onhoverscrolling="false"
	    thumbs_scrollbuttons="false"
	    thumbs_scrollindicator="false"
	    thumbs_loop="false"
	    tooltips_thumbs="false"
	    tooltips_hotspots="false"
	    tooltips_mapspots="false"
	    loadscene_flags="MERGE"
	    loadscene_blend="BLEND(0.5)"
	    controlbar_offset="20"
	/>
	<!-- set optional skin logo url -->

	<style name="skin_hotspotstyle1"
        scale="0.5"
        zorder="50"
        handcursor="true"
        onhover="showtext(Disponible · Ver ficha);"
        alpha="1"
        type="text"
        edge="center"
        css="font-family:Din Alternate; font-size:23px; color:#000000;font-weight: bold; text-align:center;"
        vcenter="true"
        padding="5"
        bg="true"
        bgcolor="0xFFFFFF"
        bgalpha="1.0"
        bgborder="0"
        bgroundedge="23"
        bgshadow=""
        txtshadow=""
        width="46" height="46"
    />
	<style name="spot-andrea" scale="0.4" zorder="50" edge="bottom" align="bottom" alpha="1"
        capture="false" handcursor="false"
	    fillcolor="0xffff00" fillalpha="0.30"
	    bordercolor="0xffff00" borderalpha="0.80"
	    onover="tween(alpha, 0.1, 0.25);"
	    onout="tween(alpha, 1.0, 0.25);"
	    ondown.touch="asyncloop(pressed, onhover(); );"
	    onup.touch="onout();"
    />
     
    <style name="skin_hotspotstyle4" scale="0.5" handcursor="false" edge="bottomleft" align="bottom" alpha="1"/>
  	<style name="hs" scale="0.4" zorder="50" edge="bottom" align="bottom" alpha="1"/>
	<style name="lupa" scale="0.6" distorted="false" oy="0" handcursor="true" zorder="50" />
	<textstyle name="default"
		font="Helvetica" fontsize.no-mobile="14" fontsize.mobile="14" bold="false"
		background="true" backgroundcolor="0x211915" backgroundalpha="0.667"
		border="true" borderwidth="2" bordercolor="0xFFFFFF" roundedge="1"
		textcolor="0xFFFFFF"
		padding="6 10"
		textalign="center"
		yoffset.no-touch="-5"
		yoffset.touch="-40"
	/>

	<action name="startup">
		if(startscene === null, copy(startscene,scene[0].name));
		loadscene(get(startscene), null, MERGE);
		
	</action>

	<scene name="scene_master" title="Masterplan 360º I" onstart="mostrar_btn_vistageneral(false);activatespot(spot-master,230);js('loadSpots()');" thumburl="panos/master.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="312.791" vlookat="27.521" fovtype="MFOV" fov="105.000" maxpixelzoom="2.0" fovmin="70" fovmax="130" limitview="range" hlookatmin="-180" hlookatmax="180" vlookatmin="-33" vlookatmax="90"/>

		<preview url="panos/master.tiles/preview.jpg" />

		<image>
			<cube url="panos/master.tiles/pano_%s.jpg" />
		</image>

		<!-- place your scene hotspots here -->
	
		<hotspot name="spot1" handcursor="false" style="spot-andrea" url="skin/spots/acceso.png" onhover="showtext(Acceso);" ath="-39.972" atv="36.156"/>
		<hotspot name="vista2" handcursor="true" zorder="50" style="skin_hotspotstyle2" url="skin/spots/spot0.png" onclick="looktohotspot(vista2, 70, tween(easeOutQuad,3));loadscene(scene_master1, null, MERGE, BLEND(1));" onhover="showtext(Ver Masterplan 360º Vista 2);" ath="130.856" atv="21.737"/>
		
		<hotspot name="hito1" style="skin_hotspotstyle4" url="skin/spots/1.png" ath="160.004" atv="-0.563"/>
