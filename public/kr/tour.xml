		<hotspot name="hito6" style="skin_hotspotstyle4" url="skin/spots/6.png" ath="-142.016" atv="0.749"/>
		<hotspot name="hito7" style="skin_hotspotstyle4" url="skin/spots/7.png" ath="-79.182" atv="1.072"/>
		<hotspot name="hito8" style="skin_hotspotstyle4" url="skin/spots/8.png" ath="-133.253" atv="0.921"/>
		<hotspot name="hito9" style="skin_hotspotstyle4" url="skin/spots/9.png" ath="-15.403" atv="0.303"/>s
		<hotspot name="vista1" handcursor="true" zorder="50" style="skin_hotspotstyle2" url="skin/spots/spot0.png" onclick="looktohotspot(vista1, 70, tween(easeOutQuad,3));loadscene(scene_master, null, MERGE, BLEND(1));" onhover="showtext(Ver Masterplan 360º Vista 1);" ath="3.555" atv="17.364"/>
		<hotspot name="spot1" handcursor="false" style="spot-andrea" url="skin/spots/parque.png" onhover="showtext(Plaza);" ath="-12.482" atv="43.012"/>
     	<hotspot name="spot2" handcursor="false" style="spot-andrea" url="skin/spots/equipamiento.png" onhover="showtext(Edificio Público);" ath="27.759" atv="51.039"/>
	</scene>
















	


	


<style name="skin_hotspotstyle2" url="skin/spots/spot0.png" crop="0|0|64|64" framewidth="64" frameheight="64" frame="0" lastframe="19" scale="0.8" onloaded="hotspot_animate();"/>
<action name="hotspot_animate"> inc(frame,1,get(lastframe),0); mul(ypos,frame,frameheight); txtadd(crop,'0|',get(ypos),'|',get(framewidth),'|',get(frameheight)); delayedcall(0.03, if(loaded, hotspot_animate() ) ); </action>


	




