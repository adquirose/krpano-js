		<hotspot name="hito5" style="skin_hotspotstyle4" url="skin/spots/5.png" ath="145.756" atv="-0.531"/>
		<hotspot name="hito6" style="skin_hotspotstyle4" url="skin/spots/6.png" ath="162.610" atv="-0.778"/>
		<hotspot name="hito7" style="skin_hotspotstyle4" url="skin/spots/7.png" ath="-133.595" atv="1.944"/>
		<hotspot name="hito8" style="skin_hotspotstyle4" url="skin/spots/8.png" ath="169.883" atv="-0.177"/>
		<hotspot name="hito9" style="skin_hotspotstyle4" url="skin/spots/9.png" ath="-87.433" atv="2.223"/>

	</scene>

	<scene name="scene_master1" title="Masterplan 360º II" onstart="mostrar_btn_vistageneral(false);activatespot(spot-master2,180);generar_spots(170,191);generar_spots(193,208);generar_spots(212,330);" thumburl="panos/master2.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="366.638" vlookat="26.868" fovtype="MFOV" fov="105.000" maxpixelzoom="2.0" fovmin="70" fovmax="130" limitview="range" hlookatmin="-180" hlookatmax="180" vlookatmin="-33" vlookatmax="90"/>

		<preview url="panos/master2.tiles/preview.jpg" />

		<image>
			<cube url="panos/master2.tiles/pano_%s.jpg" />
		</image>

		<!-- place your scene hotspots here -->
	
		<hotspot name="hito1" style="skin_hotspotstyle4" url="skin/spots/1.png" ath="-147.258" atv="0.441"/>
		<hotspot name="hito3" style="skin_hotspotstyle4" url="skin/spots/3.png" ath="130.637" atv="1.692"/>
		<hotspot name="hito2" style="skin_hotspotstyle4" url="skin/spots/2.png" ath="70.101" atv="4.724"/>
